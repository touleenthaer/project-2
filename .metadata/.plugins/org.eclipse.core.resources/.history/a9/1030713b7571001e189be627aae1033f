package magentoLab;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.annotations.BeforeTest;

public class magentolab {
	WebDriver driver =new ChromeDriver();
	
	@BeforeTest
	public void mySetup() {
		driver.get("https://magento.softwaretestingboard.com/customer/account/create/");
		
		driver.findElement(By.xpath("/html/body/div[2]/header/div[1]/div/ul/li[3]/a")).click();
		
		driver.findElement(By.xpath("//*[@id=\"firstname\"]")).sendKeys("Touleen");
		driver.findElement(By.xpath("//*[@id=\"lastname\"]")).sendKeys("Musa");
		driver.findElement(By.xpath("//*[@id=\"email_address\"]")).sendKeys("touleensinan@gmail.com");
		driver.findElement(By.xpath("//*[@id=\"password\"]")).sendKeys("ttooleen");
		driver.findElement(By.xpath("//*[@id=\"password-confirmation\"]")).sendKeys("ttooleen");
		driver.findElement(By.xpath("//*[@id=\"form-validate\"]/div/div[1]/button/span")).click();
		
		
		
		}
	
	

}
